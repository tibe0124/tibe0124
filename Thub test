local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "THub (Test Ver) ", HidePremium = false, IntroText = "THub", SaveConfig = true, ConfigFolder = "OrionTest"})
local Tab = Window:MakeTab({
	Name = "Player",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "Main"
})
Tab:AddSlider({
	Name = "Walkspeed",
	Min = 0,
	Max = 100,
	Default = 15,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "WS",
	Callback = function(Value)
	game.Workspace.Players.LocalPlayer.Character.Humanoid.Walkspeed = Value
	end    
})
Tab:AddSlider({
	Name = "JumpPower",
	Min = 0,
	Max = 200,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Height",
	Callback = function(Value)
	game.Workspace.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end    
})
Tab:AddToggle({
	Name = "Hitbox (Not Working) ",
	Default = false,
	Callback = function(Value)
	local function enableHitbox()
    hitboxEnabled = true
    for _, player in pairs(game.Players:GetPlayers()) do
        if player ~= game.Players.LocalPlayer then
            local character = player.Character
            if character and character:FindFirstChild("Head") then
                local head = character.Head
                local originalSize = head.Size
                head.Size = Vector3.new(hitboxSize, hitboxSize, hitboxSize)
                head.CanCollide = false
                local function resetHead()
                    head.Size = originalSize
                    head.CanCollide = true
                end
                player.CharacterAdded:Connect(function(newCharacter)
                    if newCharacter:FindFirstChild("Head") then
                        resetHead()
                        head = newCharacter.Head
                        originalSize = head.Size
                        head.Size = Vector3.new(hitboxSize, hitboxSize, hitboxSize)
                        head.CanCollide = false
                    end
                end)
            end
        end
    end
end
 
local function disableHitbox()
    hitboxEnabled = false
    for _, player in pairs(game.Players:GetPlayers()) do
        if player ~= game.Players.LocalPlayer then
            local character = player.Character
            if character and character:FindFirstChild("Head") then
                local head = character.Head
                head.Size = Vector3.new(2, 1, 1)
                head.CanCollide = true
            end
        end
    end
end
 
local function toggleHitbox()
    if hitboxEnabled then
        disableHitbox()
    else
        enableHitbox()
    end
end
	end



end
OrionLib:Init()
